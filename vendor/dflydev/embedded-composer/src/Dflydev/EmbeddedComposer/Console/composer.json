    "require": {
        "php": ">=5.3.2",
        "dflydev/embedded-composer-core": "1.*@dev",
        "symfony/console": "~2.3@dev",

        "composer/composer": "@dev"
    },
    "autoload": {
        "psr-0": { "Dflydev\\EmbeddedComposer\\Console": "" }
